# WebBot 开发环境配置

# 服务器配置
PORT=8081
HOST=0.0.0.0

# 文件配置
MAX_FILE_SIZE=52428800  # 50MB in bytes
UPLOAD_DIR=uploads
TEMP_DIR=uploads/temp

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=text

# 功能开关
ENABLE_LOG_PARSE=true
ENABLE_LOCK_USER=true
ENABLE_SQL_PARSE=false  # 暂未实现
ENABLE_FILE_SPLIT=false # 暂未实现
ENABLE_KYC_REVIEW=false # 暂未实现
ENABLE_REDIS_DEL=false  # 暂未实现
ENABLE_REDIS_ADD=false  # 暂未实现
ENABLE_UID_DEDUP=false  # 暂未实现

# 安全配置
ALLOWED_ORIGINS=*
CORS_ENABLED=true
RATE_LIMIT=100  # 每分钟请求数

# 清理配置
AUTO_CLEANUP=true
CLEANUP_INTERVAL=3600  # 1小时，秒为单位
FILE_RETENTION=86400   # 24小时，秒为单位